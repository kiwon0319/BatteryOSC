<?xml version="1.0" encoding="utf-8"?>
<dev.oneuiproject.oneui.layout.ToolbarLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/toolbar_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:title="Bluetooth battery"
    app:expanded="false">
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <androidx.appcompat.widget.SeslSwitchBar
                android:id="@+id/switchBar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>

            <dev.oneuiproject.oneui.widget.Separator
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>

            <dev.oneuiproject.oneui.widget.RoundLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="@color/oui_background_color">

                <com.example.batteryosc.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:TitleText="test1"/>

                <com.example.batteryosc.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:isDividerViewVisible="true"
                    app:TitleText="test2"/>

            </dev.oneuiproject.oneui.widget.RoundLinearLayout>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>

</dev.oneuiproject.oneui.layout.ToolbarLayout>